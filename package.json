{
  "name": "cursed-analytics",
  "version": "2.0.1",
  "private": true,
  "description": "Cursed Analytics",
  "license": "Apache-2.0",
  "author": "Alex Thomson",
  "main": "build/src/main.js",
  "build": {
    "appId": "io.github.lxgaming.cursedanalytics",
    "productName": "Cursed Analytics",
    "files": [
      "build/**/*",
      "public/**/*"
    ],
    "win": {
      "target": "portable",
      "icon": "public/img/icon.ico"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && cross-env NODE_ENV=development electron --remote-debugging-port=9222 .",
    "dist": "npm run lint && npm run build && electron-builder",
    "lint": "eslint src --ext .js,.ts",
    "start": "npm run build && electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "electron": "^15.1.1",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.2",
    "electron-store": "^8.0.1",
    "jquery": "^3.6.0",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.2"
  }
}
