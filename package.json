{
  "name": "cursed-analytics",
  "version": "2.0.9",
  "description": "Cursed Analytics",
  "license": "Apache-2.0",
  "author": "Alex Thomson",
  "main": "build/src/main.js",
  "build": {
    "appId": "io.github.lxgaming.cursedanalytics",
    "productName": "Cursed Analytics",
    "files": [
      "build/**/*",
      "public/**/*"
    ],
    "win": {
      "target": "portable",
      "icon": "public/img/icon.ico"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && cross-env NODE_ENV=development electron --remote-debugging-port=9222 .",
    "dist": "npm run lint && npm run build && electron-builder",
    "lint": "eslint src --ext .js,.ts",
    "start": "npm run build && electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "electron-store": "^8.2.0",
    "jquery": "^3.7.1",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.26",
    "@types/node": "^20.16.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "private": true
}
